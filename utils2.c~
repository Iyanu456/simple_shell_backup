#include "shell.h"
/**
 * _strncmp - compares the first n characters of s1 amd s2
 * @s1: first string
 * @s2: second string
 * Description: compares the first n characters of a string
 * Return: integer
 */
int _strncmp(char *s1, char *s2, int n)
{
	int i;

	for (i = 0; s1[i] && s2[i] && i < n; i++)
	{
		if (s1[i] != s2[i])
			return (s1[i] - s2[i]);
	}

	return (0);

}
