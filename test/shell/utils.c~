#include"main.h"
int _strcmp(const char *str1, const char *str2)
{
	int i, len_1 = 0, len_2 = 0;

	while (str1[len_1] != '\0')
		len_1++;

	while (str2[len_2] != '\0')
		len_2++;

	if (len_1  < len_2)
		return (-1);

	if (len_1 > len_2)
		return (1);

	if (len_1 == len_2)
	{
		for (i = 0; i < len_1; i++)
		{
			if (str1[i] == str2[i])
				continue;
			else
				return (-1);
		}
	}
	return (0);
}
